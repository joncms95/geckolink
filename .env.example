# Database (optional in development; defaults from config/database.yml)
# DATABASE_URL=postgres://localhost:5432/geckolink_development

# Redis: Sidekiq, Action Cable, and (in production) Rails cache
# REDIS_URL=redis://localhost:6379/0

# Optional: dedicated Redis for cache (production). If unset, REDIS_URL is used.
# REDIS_CACHE_URL=redis://localhost:6379/1

# Production: set master key to decrypt credentials
# RAILS_MASTER_KEY=

# Production database password (if not using DATABASE_URL)
# GECKOLINK_DATABASE_PASSWORD=
